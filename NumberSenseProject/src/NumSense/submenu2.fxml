<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="NumSense.submenu2Controller"
            prefHeight="400.0" prefWidth="600.0">
    <GridPane>
        <Button fx:id= "b0" text="Multiply 2x2 digit" GridPane.columnIndex="0" GridPane.rowIndex="0" onAction="#handle0"/>
        <Button fx:id= "b1" text="Multiply by reverse digits" GridPane.columnIndex="1" GridPane.rowIndex="0" onAction="#handle1"/>
        <Button fx:id= "b2" text="Squaring" GridPane.columnIndex="0" GridPane.rowIndex="1" onAction="#handle2"/>
        <Button fx:id= "b3" text="TBI" GridPane.columnIndex="1" GridPane.rowIndex="1" onAction="#handle2"/>

    </GridPane>

</AnchorPane>
